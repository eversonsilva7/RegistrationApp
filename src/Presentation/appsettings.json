{
	"Logging": {
		"LogLevel": {
			"Default": "Information",
			"Microsoft": "Warning",
			"Microsoft.Hosting.Lifetime": "Information"
		},
		"File": {
			"Path": "logs/myapp.log",
			"IncludeScopes": false,
			"Append": true
		}
	},
	"ConnectionStrings": {
		"DataBaseConnection": "Data Source=.;Initial Catalog=Registration;Integrated Security=True;"
	},
	"AllowedHosts": "*"
}
